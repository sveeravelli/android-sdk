<resources>
    <style name="LabelView" parent="android:style/Widget.TextView">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/video_title_size</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="AddWatch" parent="android:Widget.CompoundButton">
        <item name="android:button">@drawable/btn_add_watch</item>
        <item name="android:background">@drawable/btn_check_label_background</item>
    </style>

    <style name="VideoTitle" parent="android:style/Widget.TextView">
        <item name="android:paddingLeft">3dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/video_title_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>

    </style>


</resources>