<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    android:installLocation="preferExternal"
    package="com.example.secureplayer"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="7" android:targetSdkVersion="11" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <application
        android:debuggable="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name" >
        <activity
            android:name=".DxApiDemosActivity"
            android:configChanges="keyboardHidden|orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".apis.PersonalizationActivity"
            android:configChanges="keyboardHidden|orientation"
            android:label="Perform Personalization"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="com.discretix.PERSONALIZATION" />

                <category android:name="android.intent.category.SAMPLE_CODE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".apis.DeletePersonalizationActivity"
            android:configChanges="keyboardHidden|orientation"
            android:label="Delete Personalization"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="com.discretix.DELETE_PERSONALIZATION" />

                <category android:name="android.intent.category.SAMPLE_CODE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".apis.InitiatorActivity"
            android:configChanges="keyboardHidden|orientation"
            android:label="Initiator Acquisition"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="com.discretix.INITIATOR" />

                <category android:name="android.intent.category.SAMPLE_CODE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".apis.AcquireRightsActivity"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="com.discretix.RENEW_RIGHTS" />

                <category android:name="android.intent.category.SAMPLE_CODE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".apis.ContentDownloadActivity"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="com.discretix.DOWNLOAD_FILE" />

                <category android:name="android.intent.category.SAMPLE_CODE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".apis.DisplayRightsInfoActivity"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="com.discretix.LOCALIZE" />

                <category android:name="android.intent.category.SAMPLE_CODE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity 
            android:name=".obsolete.PlaybackActivity"
            android:screenOrientation="landscape"
            android:configChanges="orientation|keyboardHidden" >
           
            <intent-filter>
                <action android:name="com.discretix.PLAY_STREAMING" />

                <category android:name="android.intent.category.SAMPLE_CODE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".apis.ProgressiveDownload" >
            <intent-filter>
                <action android:name="com.discretix.PLAY_PD" />

                <category android:name="android.intent.category.SAMPLE_CODE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".apis.PlaybackOptionsActivity" >
            <intent-filter>
                <action android:name="com.discretix.PLAY_STREAMING_OPTIONS" />

                <category android:name="android.intent.category.SAMPLE_CODE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>